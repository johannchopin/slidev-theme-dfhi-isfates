{
  "name": "slidev-theme-dfhi-isfates",
  "version": "0.2.1",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev",
    "dfhi",
    "isfates"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build slides.md",
    "dev": "slidev slides.md --open",
    "export": "slidev export slides.md --output slides.pdf --per-slide"
  },
  "dependencies": {
    "@slidev/types": "^0.49.16",
    "prism-theme-vars": "^0.2.5",
    "vite-svg-loader": "^5.1.0"
  },
  "devDependencies": {
    "@slidev/cli": "^0.49.16",
    "playwright-chromium": "^1.45.1"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Verdana",
        "mono": "Fira Code"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johannchopin/slidev-theme-dfhi-isfates.git"
  }
}